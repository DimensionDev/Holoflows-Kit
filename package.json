{
  "name": "@holoflows/kit",
  "version": "0.7.0",
  "bugs": {
    "url": "https://github.com/DimensionDev/Holoflows-Kit/issues"
  },
  "repository": "https://github.com/DimensionDev/Holoflows-Kit",
  "license": "AGPL-3.0-or-later",
  "sideEffects": false,
  "main": "./umd/index.js",
  "module": "./es/index.js",
  "typings": "./es/",
  "scripts": {
    "build": "env-cmd -e production run-p clean \"build:tsc -- {@}\" build:rollup",
    "build:rollup": "rollup -c -m",
    "build:tsc": "tsc",
    "clean": "rimraf ./es ./umd",
    "doc": "run-s build:tsc doc:api doc:md",
    "doc:api": "api-extractor run --local --verbose",
    "doc:md": "api-documenter markdown -i temp -o api-documents",
    "eslint": "eslint . --ext .ts,.tsx",
    "eslint:watch": "yarn esw . --ext .ts,.tsx -w --color",
    "prepublishOnly": "npm run build",
    "start": "env-cmd -e development run-p \"start:tsc -- {@}\" start:rollup",
    "start:rollup": "rollup -c -w",
    "start:tsc": "tsc --watch",
    "test": "jest --watch",
    "test:ci": "jest"
  },
  "dependencies": {
    "@servie/events": "^1.0.0",
    "async-call-rpc": "^4.1.0",
    "concurrent-lock": "^1.0.7",
    "jsx-jsonml-devtools-renderer": "^1.4.3",
    "lodash-es": "^4.17.15",
    "memorize-decorator": "^0.2.2"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.8.34",
    "@microsoft/api-extractor": "^7.9.10",
    "@rollup/plugin-replace": "^2.3.1",
    "@types/jest": "^26.0.10",
    "@types/lodash-es": "^4.1.4",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "env-cmd": "^10.1.0",
    "eslint": "^7.7.0",
    "eslint-watch": "^7.0.0",
    "jest": "^26.4.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.1",
    "rollup": "^2.26.5",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "web-ext-types": "^3.2.1"
  },
  "peerDependencies": {
    "webextension-polyfill": "*"
  }
}
